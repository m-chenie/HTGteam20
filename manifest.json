{
    "manifest_version": 3, 
    "name": "MSK Centre Autofill Extension", 
    "version": "1.0", 
    "permissions": ["storage", "activeTab"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
        "matches": ["https://*/*"],
        "js" : ["content.js"],
        "css" : ["styles.css"]
    
    }
  ]
}